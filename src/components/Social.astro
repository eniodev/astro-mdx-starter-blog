---
import { Icon } from "astro-icon/components";
const { icon, username } = Astro.props;
---

<button class="flex items-center justify-center border-[1px] border-gray-500 rounded-full bg-gray-100 bg-opacity-5 px-2.5">
    <Icon name={icon} />
    <span class="text-sm">.com/{username}</span>
</button>